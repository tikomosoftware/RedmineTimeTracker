<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xml:space="preserve">
    
    <!-- MainWindow -->
    <system:String x:Key="AppTitle">Redmine Time Tracker</system:String>
    <system:String x:Key="MenuSettings">設定</system:String>
    <system:String x:Key="MenuConnectionSettings">接続設定...</system:String>
    <system:String x:Key="MenuView">表示</system:String>
    <system:String x:Key="MenuShowLog">通信ログを表示</system:String>
    <system:String x:Key="MenuHelp">ヘルプ</system:String>
    <system:String x:Key="MenuUserGuide">ユーザーガイド</system:String>
    <system:String x:Key="MenuReadme">README</system:String>
    <system:String x:Key="MenuAbout">バージョン情報</system:String>
    <system:String x:Key="LabelTargetMonth">対象月:</system:String>
    <system:String x:Key="BtnRefresh">↻ 最新情報取得</system:String>
    <system:String x:Key="StatusReady">準備完了</system:String>
    <system:String x:Key="LabelWorkTemplates">作業テンプレート</system:String>
    <system:String x:Key="BtnAddTemplate">+ 追加</system:String>
    <system:String x:Key="HeaderName">名前</system:String>
    <system:String x:Key="HeaderIssueId">チケットID</system:String>
    <system:String x:Key="HeaderHours">時間(h)</system:String>
    <system:String x:Key="HeaderActivity">作業分類</system:String>
    <system:String x:Key="HeaderFrequency">頻度</system:String>
    <system:String x:Key="HeaderAction">操作</system:String>
    <system:String x:Key="BtnEdit">編集</system:String>
    <system:String x:Key="BtnDelete">削除</system:String>
    <system:String x:Key="LabelTotal">合計: {0:F1}h</system:String>
    <system:String x:Key="BtnBulkRegister">月全体に一括登録</system:String>
    <system:String x:Key="BtnPreview">プレビュー表示</system:String>
    <system:String x:Key="LabelLog">通信ログ</system:String>
    <system:String x:Key="BtnClearLog">クリア</system:String>

    <!-- SettingsDialog -->
    <system:String x:Key="SettingsTitle">設定</system:String>
    <system:String x:Key="LabelRedmineUrl">Redmine URL:</system:String>
    <system:String x:Key="LabelApiKey">API Key:</system:String>
    <system:String x:Key="LabelApiKeyWarning">※ API Keyはローカルに平文で保存されます</system:String>
    <system:String x:Key="BtnTestConnection">接続テスト</system:String>
    <system:String x:Key="BtnSave">保存</system:String>
    <system:String x:Key="BtnCancel">キャンセル</system:String>
    <system:String x:Key="MsgConnectionSuccess">接続に成功しました！</system:String>
    <system:String x:Key="MsgConnectionFailed">接続に失敗しました: {0}</system:String>
    <system:String x:Key="MsgEnterUrlKey">URLとAPI Keyを入力してください。</system:String>

    <!-- TemplateDialog -->
    <system:String x:Key="TemplateTitleAdd">テンプレート追加</system:String>
    <system:String x:Key="TemplateTitleEdit">テンプレート編集</system:String>
    <system:String x:Key="LabelName">名前:</system:String>
    <system:String x:Key="LabelTicketId">チケットID:</system:String>
    <system:String x:Key="LabelHours">時間(h):</system:String>
    <system:String x:Key="LabelActivity">作業分類:</system:String>
    <system:String x:Key="LabelFrequency">頻度:</system:String>
    <system:String x:Key="LabelTargetDays">対象曜日:</system:String>
    <system:String x:Key="LabelTargetDay">対象日:</system:String>
    <system:String x:Key="FreqDaily">毎日 (月～金)</system:String>
    <system:String x:Key="FreqWeekly">毎週</system:String>
    <system:String x:Key="FreqMonthly">月次</system:String>
    <system:String x:Key="DayEnd4Month">月末</system:String>
    <system:String x:Key="MsgEnterName">名前を入力してください。</system:String>
    <system:String x:Key="MsgEnterTicketId">チケットIDを正しく入力してください。</system:String>
    <system:String x:Key="MsgSelectHours">時間を選択してください。</system:String>
    <system:String x:Key="MsgSelectActivity">作業分類を選択してください。</system:String>
    <system:String x:Key="MsgSelectDay">曜日を少なくとも1つ選択してください。</system:String>
    <system:String x:Key="MsgDuplicateId">チケットID #{0} は既に以下のテンプレートで使用されています:&#x0a;{1}&#x0a;&#x0a;入力ミスではありませんか？&#x0a;そのまま保存しますか？</system:String>
    <system:String x:Key="TitleDuplicate">チケットID重複</system:String>

    <!-- Logic Messages -->
    <system:String x:Key="MsgConfirmDelete">'{0}' を削除してもよろしいですか？</system:String>
    <system:String x:Key="TitleConfirm">確認</system:String>
    <system:String x:Key="MsgBulkRegisterConfirm">{0} のタスクを一括登録しますか？&#x0a;&#x0a;対象日数: {1} 日</system:String>
    <system:String x:Key="MsgBulkRegisterSuccess">一括登録が完了しました！</system:String>
    <system:String x:Key="MsgSelectMonth">月を選択してください。</system:String>
    <system:String x:Key="MsgNoTemplates">有効なテンプレートがありません。</system:String>

    <!-- Code-Behind Extras -->
    <system:String x:Key="DateFormatMonth">yyyy年M月</system:String>
    <system:String x:Key="MsgConnectionRequired">接続設定が必要です。&#x0a;メニュー＞設定＞接続設定から設定してください。</system:String> 
    <system:String x:Key="StatusLoading">読込中...</system:String>
    <system:String x:Key="StatusError">エラー: {0}</system:String>
    <system:String x:Key="StatusReadyCount">準備完了 ({0}テンプレート, {1}件登録済み)</system:String>
    
    <system:String x:Key="MenuUnexcludeDate">除外を解除する</system:String>
    <system:String x:Key="MenuExcludeDate">休日に設定する（登録スキップ）</system:String>
    <system:String x:Key="MenuSetWorkDay">出勤日に設定する</system:String>
    <system:String x:Key="MenuUnsetWorkDay">出勤日設定を解除する</system:String>
    <system:String x:Key="MsgSetWorkDay">{0} を出勤日に設定しました</system:String>
    <system:String x:Key="MsgUnsetWorkDay">{0} の出勤日設定を解除しました</system:String>
    
    <system:String x:Key="StatusHoliday">休日</system:String>
    <system:String x:Key="StatusWorkDay">出勤日</system:String>
    <system:String x:Key="StatusUnregistered">未登録</system:String>
    
    <system:String x:Key="MsgBulkRegisterResult">一括登録完了！

新規: {0}件
上書き: {1}件
スキップ: {2}日
エラー: {3}件</system:String>
    <system:String x:Key="TitleResult">結果</system:String>
    <system:String x:Key="TitleInfo">情報</system:String>
    <system:String x:Key="TitleSuccess">成功</system:String>
    <system:String x:Key="TitleError">エラー</system:String>
    
    <system:String x:Key="MsgEnterUrl">Redmine URLを入力してください。</system:String>
    <system:String x:Key="MsgEnterApiKey">API Keyを入力してください。</system:String>
    

    <system:String x:Key="MsgEnterIssueId">チケットIDを入力してください。</system:String>
    <system:String x:Key="MsgEnterHours">有効な時間(h)を入力してください。</system:String>

    <system:String x:Key="MsgSelectFrequency">頻度を選択してください。</system:String>
    

    <system:String x:Key="DayEndofMonth">月末</system:String>

    <system:String x:Key="LogSettingsUpdated">設定更新: {0}</system:String>
    <system:String x:Key="LogDataLoadStart">--- データ読込開始 ---</system:String>
    <system:String x:Key="LogTemplateLoaded">テンプレート読込: {0}件</system:String>
    <system:String x:Key="LogActivityLoaded">作業分類取得: {0}件</system:String>
    <system:String x:Key="LogTimeEntryLoaded">既存時間取得: {0}件</system:String>
    <system:String x:Key="LogDataLoadEnd">--- データ読込完了 ---</system:String>
    <system:String x:Key="LogOverwriteComplete">  → 上書き完了</system:String>
    <system:String x:Key="LogRegisterComplete">  → 新規登録完了</system:String>
    <system:String x:Key="LogLoadError">データ読込エラー: {0}</system:String>
    <system:String x:Key="LogSaveWorkDayError">出社日設定の保存エラー: {0}</system:String>
    <system:String x:Key="LogSaveExcludeDateError">除外日設定の保存エラー: {0}</system:String>
    <system:String x:Key="LogTimeEntryLoadError">既存時間取得エラー: {0}</system:String>
    
    <system:String x:Key="PreviewTitle">== {0} 登録プレビュー ==&#x0a;</system:String>
    <system:String x:Key="PreviewTotal">合計: {0}件, {1}h</system:String>
    <system:String x:Key="TitlePreview">プレビュー</system:String>

    <system:String x:Key="DayMon">月</system:String>
    <system:String x:Key="DayTue">火</system:String>
    <system:String x:Key="DayWed">水</system:String>
    <system:String x:Key="DayThu">木</system:String>
    <system:String x:Key="DayFri">金</system:String>
    <system:String x:Key="DaySat">土</system:String>
    <system:String x:Key="DaySun">日</system:String>

</ResourceDictionary>
